{
	"name":"bugbuster/errorlog_enhanced",
	"description":"Contao error.log with complete relative file paths.",
	"keywords":["contao","error","log"],
	"type":"contao-module",
	"homepage":"http://contao.ninja",
	"license":"LGPL-3.0+",
	"authors":[
		{
			"name":"Glen Langer",
			"homepage":"http://contao.ninja",
			"role":"Developer"
		}
	],
	"support":{
		"issues":"https://github.com/BugBuster1701/errorlog_enhanced/issues",
		"irc":"irc://irc.freenode.org/contao.dev",
		"source":"https://github.com/BugBuster1701/errorlog_enhanced"
	},
	"require":{
		"php":">=5.3",
		"contao/core":">=3.5,<4",
		"contao-community-alliance/composer-plugin": "*"
	},
	"autoload":{
		"classmap":[""]
	},
	"replace":{
		"contao-legacy/errorlog_enhanced":"*"
