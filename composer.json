	"extra":{
		"contao":{
			"sources":{
				"":"system/modules/errorlog_enhanced"
			},
			"transifex": {
				"project": "contao-errorlog_enhanced",
				"prefix": "master-",
				"languages_cto": "languages",
				"languages_tx": ".tx"
			}
		},
		"branch-alias": {
			"dev-develop": "1.0.x-dev"
		}
	}
